{
  "rules": {
    ".read": true,
    "ratings": {
      ".read": false,
      ".write": false,
      "$uid": {
        ".read": "auth.uid === $uid",
        ".write": "auth.uid === $uid"
      }
    },
    "speakers": {
      ".read": true,
      ".write": "root.child('users').child(auth.uid).child('rules').child('speakers').val() === true"
    },
    "partners":{
    	".read": true,
    	".write": false
    },
    "schedule":{
    	".read": true,
    	".write": false
    },
    "sessions":{
    	".read": true,
    	".write": false
    },
    "resources":{
    	".read": true,
    	".write": false
    },
    "generated": {
      ".read": true,
      ".write": false
    },
    "userSessions": {
      ".read": false,
      ".write": false,
      "$uid": {
        ".read": "auth.uid === $uid",
        ".write": "auth.uid === $uid"
      }
    },
    "cdhPrivateKey":{
    	".read": true,
    	".write": false
    },
    "maplayer":{
    	".read": true,
    	".write": false
    },
    "tags":{
    	".read": true,
    	".write": false
    },
    "userFeedbacks":{
    	"$uid": {
      	".read": "auth.uid === $uid",
      	".write": "auth.uid === $uid"
     	}
    },
    "users":{
    	"$uid": {
      	".read": true,
      	".write": "auth.uid === $uid"
     	}
    },
    "cdhProgress":{
    	"$uid": {
      	".read": "auth.uid === $uid",
      	".write": "auth.uid === $uid"
     	}
    }
  }
}
